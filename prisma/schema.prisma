datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Stock {
  id          Int     @id @default(autoincrement())
  stock_id    String
  stock_name  String
  price       Float
  quantity    Int
}